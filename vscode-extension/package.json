{
    "name": "openkombai-vscode",
    "displayName": "OpenKombai",
    "description": "Generate React code from UI screenshots using your local OpenKombai backend.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.90.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:openkombai.generate"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "openkombai.generate",
                "title": "OpenKombai: Generate Code from Image"
            }
        ],
        "configuration": {
            "title": "OpenKombai",
            "properties": {
                "openkombai.backendUrl": {
                    "type": "string",
                    "default": "http://localhost:8000",
                    "description": "URL of the local OpenKombai backend."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.90.0",
        "@types/mocha": "^10.0.6",
        "@types/node": "20.x",
        "@typescript-eslint/eslint-plugin": "^7.11.0",
        "@typescript-eslint/parser": "^7.11.0",
        "eslint": "^8.57.0",
        "mocha": "^10.4.0",
        "typescript": "^5.4.5",
        "@vscode/test-electron": "^2.4.0"
    },
    "dependencies": {
        "axios": "^1.7.2",
        "form-data": "^4.0.0"
    }
}